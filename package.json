{
  "name": "approveman",
  "version": "1.0.0",
  "private": true,
  "description": "A GitHub app to help approve pull requests.",
  "author": "Tianhao Zhou <tianhaoz@umich.edu>",
  "license": "ISC",
  "repository": "https://github.com/tianhaoz95/approveman.git",
  "homepage": "https://github.com/tianhaoz95/approveman",
  "bugs": "https://github.com/tianhaoz95/approveman/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "dev": "npm run build:watch",
    "start": "probot run ./lib/src/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:silent": "LOG_LEVEL=warn jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@types/minimatch": "^3.0.3",
    "@types/mustache": "^4.0.1",
    "@types/node": "^14.6.0",
    "minimatch": "^3.0.4",
    "mustache": "^4.0.1",
    "probot": "^9.5.3"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/nock": "^11.1.0",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.9.0",
    "nock": "^12.0.0",
    "nodemon": "^2.0.0",
    "smee-client": "^1.1.0",
    "standard": "^14.3.1",
    "ts-jest": "^25.1.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
